{
	"name": "nanosnippets",
	"publisher": "holdesher",
	"displayName": "Nano Snippets",
	"description": "Snippets for various technologies and languages, taking into account all modern trends and rules.",
	"repository": {
		"type": "git",
		"url": "https://github.com/holdesher/Nano-Snippets"
	},
	"type": "module",
	"version": "0.0.35",
	"engines": {
		"vscode": "^1.73.0"
	},
	"preview": false,
	"extensionPack": [
		"kah3vich.holdesher"
	],
	"categories": [
		"Extension Packs",
		"Snippets"
	],
	"galleryBanner": {
		"color": "#20232A",
		"theme": "dark"
	},
	"icon": "assets/img/preview.png",
	"license": "Apache-2.0",
	"author": {
		"name": "holdesher",
		"url": "https://github.com/Holdesher",
		"email": "holdesher@ro.ru"
	},
	"bugs": {
		"url": "https://github.com/holdesher/nano-snippets/issues",
		"email": "holdesher@ro.ru"
	},
	"keywords": [
		"kah3vich",
		"holdesher",
		"nano",
		"snippets",
		"nanosnippets",
		"snippet",
		"snippets",
		"react",
		"html",
		"ts",
		"js",
		"typescript",
		"javascript"
	],
	"main": "./out/extension.js",
	"browser": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/output/html.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/output/javascript.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/output/javascript.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/output/typescript.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/output/typescript.json"
			},
			{
				"language": "python",
				"path": "./snippets/output/python.json"
			}
		],
		"commands": [
			{
				"command": "nano-snippets",
				"title": "Nano Snippets"
			}
		]
	},
	"activationEvents": [
		"onCommand:nano-snippets"
	],
	"homepage": "https://github.com/holdesher/nano-snippets/readme.md",
	"dependencies": {
		"vsce": "^2.15.0"
	},
	"devDependencies": {
		"@types/node": "^22.7.7",
		"@types/vscode": "^1.63.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3"
	},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"vsix": "rm -rf *.vsix",
		"snippets": "node ./scripts/snippets.js",
		"vscode:prepublish": "npm run vsix && npm run snippets && npm run compile",
		"build": "vsce package",
		"prepare": "husky"
	}
}
